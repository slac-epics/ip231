  field(OUT, "#C$(CARD) S0 @")
  field(ENAB, "Enabled")
  field(ENM, "$(ID)")
  field(OUT0, "$(P0)")
  field(OUT1, "$(P1)")
  field(OUT2, "$(P2)")
  field(OUT3, "$(P3)")
  field(OUT4, "$(P4)")
  field(OUT5, "$(P5)")
  field(OUT6, "$(P6)")
  field(OUT7, "$(P7)")
  field(OUT8, "$(P8)")
  field(OUT9, "$(P9)")
  field(OUTA, "$(P10)")
  field(OUTB, "$(P11)")
  field(OUTC, "$(P12)")
  field(OUTD, "$(P13)")
  field(VME,  "$(VME)")
}
