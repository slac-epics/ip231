record(ai, "$(dev):URER") {
  field(DESC, "URER")
  field(SCAN, "Event")
  field(EVNT, 140)
  field(INP, "$(ioc):PULSEID")
  field(FLNK, "$(dev):UIMR")
  field(PREC, "4")
  field(LOPR, "-12")
  field(HOPR, "+12")
}

