record(sub, "$(DEVICE):MPSCTRL") {
  field(INAM, "mpsEvrProcInit")
  field(SNAM, "mpsEvrProc")
# INPD,H,L are spare for future use.
  field(INPA, "$(DEVICE):PATTERN360HZ.C")
  field(INPB, "$(DEVICE):PATTERN360HZ.L")
  field(INPC, "$(DEVICE):PATTERN360HZ.H")
  field(INPE, "$(DEVICE):PATTERNN-1.C")
  field(INPF, "$(DEVICE):PATTERNN-1.L")
  field(INPG, "$(DEVICE):PATTERNN-1.H")
  field(INPI, "$(DEVICE):PATTERNN-2.C")
  field(INPJ, "$(DEVICE):PATTERNN-2.L")
  field(INPK, "$(DEVICE):PATTERNN-2.H")
  field(BRSV, "INVALID")
}
