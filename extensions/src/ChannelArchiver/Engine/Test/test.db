#! env softIoc -s -d test.db
# Example Database for testing the Engine

record(calc, "ramp1")
{
   field(SCAN, "1 second")
   field(INPA, "ramp1")
   field(CALC, "A<10?A+1:0")
}

record(calc, "ramp2")
{
   field(SCAN, "2 second")
   field(INPA, "ramp2")
   field(CALC, "A<10?A+1:0")
}

record(calc, "ramp2_threshold")
{
   field(INPA, "ramp1 CP")
   field(CALC, "A >= 9")
}
