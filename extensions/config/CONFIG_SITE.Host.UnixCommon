# java definitions
CLASSPATH = -classpath $(INSTALL_JAVA)
SOURCEPATH = -sourcepath ..
JAVACCMD = $(subst \,/,javac $(JAVAC_FLAGS) )
JAVAHCMD = $(subst \,/,javah -jni $(JAVAH_FLAGS))
JARCMD = $(subst \,/,jar $(JAR_OPTIONS) $(MANIFEST) $(JAR) $(JAR_INPUT))

